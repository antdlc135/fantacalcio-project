{
	"name": "ai-fantamaster",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"devel": "npm-run-all --parallel dev nuv",
		"nuv": "nuv ide watch",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
