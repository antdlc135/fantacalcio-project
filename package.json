		"animejs": "^3.2.2",
		"axios": "^1.6.2",
		"esbuild": "^0.20.1",
		"marked": "^13.0.2",
		"npm-run-all": "^4.1.5",
		"openai": "^4.20.1",
		"scrollreveal": "^4.0.9",
		"svelte-routing": "^2.12.0",
		"swiper": "^11.1.3"
	},
	"nuvolaris": {
		"devel": "npm run dev",
		"deploy": "npm run build"
	}
